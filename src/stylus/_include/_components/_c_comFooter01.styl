footer.c-comFooter01
	
	.navBlock
		+MQ_PC(SP_RES_WID)
			padding 10px
			background-color #dfdfdf
		.footerNav
			+MQ_SP(SP_RES_WID)
				&>ul
					&>li
						width 100%
						&>a
							padding 15px
							background-color #dfdfdf
		.childNav
			&>ul
				+MQ_PC(SP_RES_WID)
					padding-left 1.0em
				+MQ_SP(SP_RES_WID)
					padding 15px
				&>li
					&>a
						fontSize(12)
						
		a
			display block
			color inherit
			text-decoration none
	.copyBlock
		padding 15px
		background-color #cfcfcf
		fontSize(11)